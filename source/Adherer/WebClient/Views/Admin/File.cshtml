
@{
    ViewData["Title"] = "File";
    Layout = "~/Views/Shared/LayoutAdmin.cshtml";
}
@section Css{
    <link rel="stylesheet" type="text/css" href="~/css/admin/adfile.css">
}
<div class="k main-form">
    <div class="k body-form">
        <span class=" k t name-file">Thông tin hồ sơ Đảng viên</span>

        <div class="k f-bd-file">
            <div class="k row-item">
                <div class="k tab-avt">
                    <div class=" k img-avt-file" id="img-avt">
                        <span class="brower-avt" onclick="getImage()">Brower</span>
                        <input id="multi-file" class="hidden" style="display:none" type="file" accept="image/*" multiple="">
                    </div>
                    <div class="k f-t-avt">
                        <span class="k t t-avt-left">Mã Đảng viên:</span>
                        <input type="text" class="k t ip-avt-t" disabled="disabled" id="madv"/>
                    </div>
                    <div class="k f-t-avt">
                        <span class="k t t-avt-left">Họ tên Đảng viên:</span>
                        <input type="text" class="k t ip-avt-t" id="namedv" disabled="disabled" />
                    </div>
                </div>
                <div class="k bd-tab-body">
                    <div class="k tab-body">
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Họ tên khai sinh:</span>
                            <input type="text" class="k t ip-avt-t" id="namekhaisinh" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Họ tên đang dùng:</span>
                            <input type="text" class="k t ip-avt-t" id="namedangdung" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Ngày tháng năm sinh:</span>
                            <div class="form-group">
                                <div class='input-group date' id='datepicker-birthday'>
                                    <input type='text' class="form-control" id="birthday" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Giới tính:</span>
                            <select class="k t ip-avt-t" id="sl-giotinh">
                                <option value="0">Nam</option>
                                <option value="1">Nữ</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Dân tộc:</span>
                            <select class="k t ip-avt-t" id="sl-nation">
                                @*<option value="1">Kinh</option>
                    <option value="1">Tày</option>*@
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Tôn giáo:</span>
                            <input type="text" class="k t ip-avt-t" id="tongiao" />
                        </div>

                        <div class="k row-item-body">
                            <span class="k t t-avt-left">CMND:</span>
                            <input type="text" class="k t ip-avt-t" maxlength="12" id="cmnd" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Nơi cấp:</span>
                            <input type="text" class="k t ip-avt-t" id="noicapcmnd" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Quê quán:</span>
                            <input type="text" class="k t ip-avt-t" id="quequan" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Hộ khẩu thường trú:</span>
                            <input type="text" class="k t ip-avt-t" id="hokhauthuongtru" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Hôn nhân:</span>
                            <select class="k t ip-avt-t" id="sl-honnhan">
                                <option value="0">Đã có gia đình</option>
                                <option value="1">Chưa có gia đình</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Sức khỏe:</span>
                            <input type="text" class="k t ip-avt-t" id="suckhoe" />
                        </div>



                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Nghề nghiệp:</span>
                            <input type="text" class="k t ip-avt-t" id="nghenghiep" />
                        </div>

                    </div>
                    @*--------------------------------------------------------------------------------------------*@
                    <div class="k tab-body">
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Đơn vị:</span>
                            <select class="k t ip-avt-t" id="sl-org">
                                @*<option value="1">Phòng Giải pháp</option>
                    <option value="1">Phòng mạng</option>*@
                            </select>
                        </div>

                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Số lý lịch:</span>
                            <input type="text" class="k t ip-avt-t" id="solylich" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Số điện thoại:</span>
                            <input type="text" class="k t ip-avt-t" maxlength="9" id="sdt" />
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Email:</span>
                            <input type="text" class="k t ip-avt-t" id="email" />
                        </div>

                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Ngày vào Đảng chính thức:</span>
                            <div class="form-group">
                                <div class='input-group date' id='datepicker-vaodct'>
                                    <input type='text' class="form-control" id="ngayvaodangct" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Ngày vào Đảng dự bị:</span>
                            <div class="form-group">
                                <div class='input-group date' id='datepicker-vaoddb'>
                                    <input type='text' class="form-control" id="ngayvaodangdb" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Ngày vào Đoàn:</span>
                            <div class="form-group">
                                <div class='input-group date' id='datepicker-ngayvaodoan'>
                                    <input type='text' class="form-control" id="ngayvaodoan" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>

                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Trình độ văn hóa:</span>
                            <select class="k t ip-avt-t" id="sl-trinhdovanhoa">
                                <option value="12">Lớp 12</option>
                                <option value="11">Lớp 11</option>
                                <option value="10">Lớp 10</option>
                                <option value="9">Lớp 9</option>
                                <option value="8">Lớp 8</option>
                                <option value="7">Lớp 7</option>
                                <option value="6">Lớp 6</option>
                                <option value="5">Lớp 5</option>
                                <option value="4">Lớp 4</option>
                                <option value="3">Lớp 3</option>
                                <option value="2">Lớp 2</option>
                                <option value="1">Lớp 1</option>
                            </select>
                        </div>
                        <div class="k row-item-body">
                            <span class="k t t-avt-left">Chuyên môn:</span>
                            <input type="text" class="k t ip-avt-t" id="chuyenmon" />
                        </div>

                        <div class="k row-item-body-add">
                            <span class="k t t-avt-addres">Địa chỉ cư trú:</span>
                            <select class="k t sl-avt-add" id="sl-province">
                                @*<option value="1">Tỉnh/Thành phố</option>
                    <option value="1">HCM</option>*@
                            </select>
                            <select class="k t sl-avt-add" id="sl-district">
                                @*<option value="1">Quận/Huyện/Thị xã</option>
                    <option value="1">Tuy Phong</option>*@
                            </select>
                            <select class="k t sl-avt-add" id="sl-ward">
                                @*<option value="1">Xã/Phường/Thị trấn</option>
                    <option value="1">Cày cây</option>*@
                            </select>
                            <input type="text" class="k t ip-avt-add" id="noicutru" placeholder="Tên đường, thôn xóm, số nhà" />
                        </div>
                    </div>
                    <span class="k t err-validate">**Vui lòng kiểm tra lại thông tin nhập trước khi cập nhật</span>
                    <span class="t bnt-update-file"onclick="validateFile()">Cập nhật</span>
                </div>



            </div>
        </div>
        <div class="k t tab-note-family">
            <span class="k">Thành viên gia đình</span>
            <i class="fa fa-angle-down ic-show-ft" aria-hidden="true" onclick="changeTabFamily(this)"></i>
        </div>
        <div class="k tab-family" id="tab-family">
            <div class="k row-table">
                <span class="k t tt-table-dt bold">STT</span>
                <span class="k t tt-table-dt bold">Quan hệ</span>
                <span class="k t tt-table-dt bold">Họ tên</span>
                <span class="k t tt-table-dt bold">Nghề nghiệp</span>
                <div class="k t tt-table-dt bold">
                    Cập nhật
                </div>
            </div>
            <div class="k row-table">
                <span class="k t tt-table-dt">1</span>
                <span class="k t tt-table-dt">Con</span>
                <span class="k t tt-table-dt">Nguyễn Văn Cha</span>
                <span class="k t tt-table-dt">Làm nông</span>
                <div class="k t tt-table-dt">
                    <i class="fa fa-cogs" data-toggle="modal" data-target="#modalinsertdangbo" onclick="showTabEditDB(1)"></i>
                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                </div>
            </div>
            <div class="k row-table">
                <span class="k t tt-table-dt">2</span>
                <span class="k t tt-table-dt">Con</span>
                <span class="k t tt-table-dt">Lê Văn Gì Đó</span>
                <span class="k t tt-table-dt">Làm nông</span>
                <div class="k t tt-table-dt">
                    <i class="fa fa-cogs" data-toggle="modal" data-target="#modalinsertdangbo" onclick="showTabEditDB(1)"></i>
                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                </div>
            </div>
        </div>

        <div class="k t tab-note-family">
            <span class="k">Khen thưởng</span>
            <i class="fa fa-angle-down ic-show-ft" aria-hidden="true" onclick="changeTabBonus(this)"></i>
        </div>
        <div class="k tab-bonus" id="tab-bonus">
            <div class="k row-table">
                <span class="k t tt-table-dt bold">STT</span>
                <span class="k t tt-table-dt bold">Khen thưởng</span>
                <span class="k t tt-table-dt bold">Đơn vị</span>
                <span class="k t tt-table-dt bold">Thời gian</span>
                <div class="k t tt-table-dt bold">
                    Cập nhật
                </div>
            </div>

            <div class="k row-table">
                <span class="k t tt-table-dt">1</span>
                <span class="k t tt-table-dt">Đi làm chuyên cần</span>
                <span class="k t tt-table-dt">Trung tâm IT</span>
                <span class="k t tt-table-dt">12/12/2012</span>
                <div class="k t tt-table-dt">
                    <i class="fa fa-cogs" data-toggle="modal" data-target="#modalinsertdangbo" onclick="showTabEditDB(1)"></i>
                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                </div>
            </div>

        </div>

        <div class="k t tab-note-family">
            <span class="k">Kỷ luật</span>
            <i class="fa fa-angle-down ic-show-ft" aria-hidden="true" onclick="changeTabDiscipline(this)"></i>
        </div>

        <div class="k tab-discipline" id="tab-boldiscipline">
            <div class="k row-table">
                <span class="k t tt-table-dt bold">STT</span>
                <span class="k t tt-table-dt bold">Kỷ luật</span>
                <span class="k t tt-table-dt bold">Đơn vị</span>
                <span class="k t tt-table-dt bold">Thời gian</span>
                <div class="k t tt-table-dt bold">
                    Cập nhật
                </div>
            </div>

            <div class="k row-table">
                <span class="k t tt-table-dt">1</span>
                <span class="k t tt-table-dt">Bàn tán xôn xao</span>
                <span class="k t tt-table-dt">Trung tâm Ngã 3</span>
                <span class="k t tt-table-dt">11/12/2012</span>
                <div class="k t tt-table-dt">
                    <i class="fa fa-cogs" data-toggle="modal" data-target="#modalinsertdangbo" onclick="showTabEditDB(1)"></i>
                    <i class="fa fa-trash-o" aria-hidden="true"></i>
                </div>
            </div>

        </div>


    </div>
</div>

@section Script{
    <script src="~/js/admin/adfile.js"></script>
    <script>
        try {
            var id = @ViewBag.usid;
            getDangVien(id, bindingFile)
        }
        catch (er) {
            alert("error");
        }
    </script>
}

